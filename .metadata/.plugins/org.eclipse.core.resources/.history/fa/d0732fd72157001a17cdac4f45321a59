package com.belejanor.switcher.scheduler;

import java.util.TimerTask;

public class TaskBatchExcuteChrono extends TimerTask{

	@Override
	public void run() {

			System.out.println("Se ejecuto Chrono!!");
			try {
				Thread.sleep(100000);
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
			System.out.println("Termino Chrono!!");
	}

}
